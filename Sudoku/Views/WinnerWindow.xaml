<Window x:Class="Sudoku.Views.WinnerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sudoku.Views"
        mc:Ignorable="d"
        Title="WinnerWindow" Height="150" Width="200" WindowStartupLocation="CenterScreen" WindowStyle="None">
    <Grid>
        <StackPanel Orientation="Vertical">
            <TextBlock Style="{StaticResource LargeText}">Solved</TextBlock>
            <TextBlock Name="TimeTextBlock"></TextBlock>
            <Button Name="CloseButtonClick" Click="CloseButtonClick_Click" Content="OK"></Button>
        </StackPanel>
    </Grid>
</Window>
